# https://atom.io/docs/latest/using-atom-snippets

'.source.js':
  'Mocha Describe':
    'prefix': 'describe'
    'body': """
      describe('$1', function() {
        $2
      })
    """
  'Mocha Context':
    'prefix': 'context'
    'body': """
      context('$1', function() {
        $2
      })
    """
  'Mocha BeforeEach':
    'prefix': 'beforeEach'
    'body': """
      beforeEach(function() {
        $1
      })
    """
  'Mocha AfterEach':
    'prefix': 'afterEach'
    'body': """
      afterEach(function() {
        $1
      })
    """
  'Mocha It':
    'prefix': 'it'
    'body': """
      it('should $1', function() {
        $2
      })
    """
